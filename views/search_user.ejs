<% array.forEach(element => { %>
    <div class="products-row delete-row<%- element._id %>">
        <button class="cell-more-button">
            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24"
                fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                stroke-linejoin="round" class="feather feather-more-vertical">
                <circle cx="12" cy="12" r="1" />
                <circle cx="12" cy="5" r="1" />
                <circle cx="12" cy="19" r="1" />
            </svg>
        </button>

        <div class="product-cell category"><span class="cell-label">Tên người dùng:</span>
            <%- element.tennguoidung %>
        </div>

        <div class="product-cell category "><span class="cell-label">Tên tài khoản:</span>
            <%- element.tentaikhoan %>
        </div>

        <div class="product-cell stock"><span class="cell-label">Tên người dùng:</span>
            <%- element.matkhau%>
        </div>
        <div class="product-cell price"><span class="cell-label">Email:</span>
            <%- element.email %>
        </div>
        <div class="product-cell sales d-flex justify-content-around"><span
                class="cell-label ">Sales:</span>
            <button  data-id="<%- element._id  %>" class="btn btn-primary btn_edit ">
                <i class="fa-solid fa-pen-to-square"></i></button>
            <button data-id="<%- element._id  %>" class="btn btn-danger btn_delete">
                <i class="fa-solid fa-trash"></i></button>
        </div>
    </div>
<% }) %>